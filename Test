git clone https://github.com/your-username/your-repo.git
npm install express
const express = require('express')

const app = express()

app.get('/', (req, res) => {

  res.send('Hello World!')

})

app.listen(3000, () => {

  console.log('Example app listening on port 3000!')

})
git add .

git commit -m "Initial commit"

git push origin main
